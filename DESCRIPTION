Package: unisets
Title: Collection of Classes to Store Gene Sets
Version: 0.99.0
Date: 2019-01-15
Authors@R: c(
    person("Kevin", "Rue-Albrecht",
        email = "kevin.rue-albrecht@kennedy.ox.ac.uk",
        role = c("aut", "cre"),
        comment = c(ORCID = "0000-0003-3899-3872")),
    person("Robert", "Amezquita", role = c("aut"))
